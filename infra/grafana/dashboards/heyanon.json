{
  "dashboard": {
    "id": null,
    "uid": null,
    "title": "HeyAnon - Basic",
    "tags": [],
    "timezone": "browser",
    "schemaVersion": 37,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "type": "graph",
        "title": "Requests per second",
        "targets": [
          {
            "expr": "sum by(path) (rate(heyanon_requests_total[1m]))",
            "legendFormat": "{{path}}",
            "refId": "A"
          }
        ],
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8}
      },
      {
        "type": "graph",
        "title": "Errors per second",
        "targets": [
          {
            "expr": "sum by(path) (rate(heyanon_errors_total[1m]))",
            "legendFormat": "{{path}}",
            "refId": "A"
          }
        ],
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8}
      },
      {
        "type": "graph",
        "title": "p95 latency (ms)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(heyanon_request_latency_seconds_bucket[1m])) by (le, path))",
            "legendFormat": "{{path}}",
            "refId": "A"
          }
        ],
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8}
      }
    ]
  },
  "overwrite": true
}
